// Defining variables

make = "Honda";
model = "Dio 110";
color = "Axis Mat Grey metallic";
year = 2017;
let ownIt = true;

// Printing result of the boolean variable ownIt in console

if (ownIt) {
  console.log("I own it!");
} else {
  console.log("I don't own it.");
}

// Calculating age of the vehicle

age = 2024 - year;

// Output of variables declared

document.writeln("Make: " + make + "<br>");
document.writeln("Model: " + model + "<br>");
document.writeln("Color: " + color + "<br>");
document.writeln("Year: " + year + "<br>");
document.writeln("Age: " + age + " years<br>");
